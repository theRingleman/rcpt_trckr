<div class="container">
  <div class="row">

    <h1 class="col s12">Edit <%= @store.name %></h1>

    <form action="/stores/<%= @store.id %>/edit" method="post" class="col s12">

      <div class="input-field col s6">
        <label for="name">Name: </label>
        <input type="text" name="name">
      </div>

      <input type="submit" value="Submit" class="btn">

    </form>

  </div>
</div>
