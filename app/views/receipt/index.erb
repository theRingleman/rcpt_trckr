<div class="container">
  <h1>Here is a list of all your current receipts.</h1>

  <ul>
    <%@receipts.each do |receipt|%>
      <li><%=receipt.store.name%> - $<%=receipt.total%> - <%= edited_date(receipt.created_at) %> - <a href="/receipts/<%= receipt.id %>">View</a></li>
    <%end%>
  </ul>
</div>
